<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.tb.dao.AccessClassMapper">
  <resultMap id="BaseResultMap" type="team.tb.pojo.AccessClass">
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="tabid" jdbcType="VARCHAR" property="tabid" />
    <result column="classid" jdbcType="VARCHAR" property="classid" />
  </resultMap>
  <insert id="insert" parameterType="team.tb.pojo.AccessClass">
    insert into t_access_class (id, tabid, classid
      )
    values (#{id,jdbcType=INTEGER}, #{tabid,jdbcType=VARCHAR}, #{classid,jdbcType=VARCHAR}
      )
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select id, tabid, classid
    from t_access_class
  </select>
</mapper>
